{
  "name": "@giraphql/plugin-prisma",
  "version": "0.10.0",
  "description": "A GiraphQL plugin for more efficient integration with prisma",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "exports": {
    "import": "./esm/index.js",
    "require": "./lib/index.js"
  },
  "scripts": {
    "generate": "prisma generate",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hayes/giraphql.git"
  },
  "bin": {
    "prisma-giraphql-types": "./bin/generator.js"
  },
  "author": "Michael Hayes",
  "license": "ISC",
  "keywords": [
    "giraphql",
    "graphql",
    "schema",
    "typescript",
    "prisma",
    "db",
    "postgres",
    "mysql",
    "sql",
    "sqlite",
    "mongo"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@prisma/generator-helper": "^3.1.1"
  },
  "prisma": {
    "seed": "pnpm ts-node prisma/seed.ts"
  },
  "peerDependencies": {
    "@prisma/client": "*",
    "graphql": ">=15.1.0"
  },
  "devDependencies": {
    "@giraphql/core": "^2.16.0",
    "@giraphql/plugin-errors": "^2.10.0",
    "@giraphql/plugin-relay": "^2.16.0",
    "@giraphql/test-utils": "^0.2.0",
    "@prisma/client": "^3.1.1",
    "graphql": "15.6.0",
    "graphql-tag": "^2.12.5",
    "prisma": "^3.1.1"
  }
}
